<%- include('../partials/header') %>

<table id="parks">
  <thead>
   <p>Some Parks</p>
  </thead>
  <tbody>
    <% parks.forEach(function(p) { %>
        <tr>
            <td><img src= "<%= p.parkImage %>"></td>
            <td><%= p.parkName %></td>
            <td><%= p.parkCode %></td>
        <td>
            <% if (user) { %>
            <a href="/parks/<%= p._id %>">Details</a>
            <% } %>
        </td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>